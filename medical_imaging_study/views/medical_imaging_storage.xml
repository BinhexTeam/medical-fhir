<odoo>
    <record model="ir.ui.view" id="medical_imaging_storage_form_view">
        <field
            name="name"
        >medical.imaging.storage.form (in medical_imaging_study)</field>
        <field name="model">medical.imaging.storage</field>
        <field name="arch" type="xml">
            <form>
                <header>
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="endpoint_ids" />
                    </group>
                </sheet>
                <div class="oe_chatter" />
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="medical_imaging_storage_search_view">
        <field
            name="name"
        >medical.imaging.storage.search (in medical_imaging_study)</field>
        <field name="model">medical.imaging.storage</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
            </search>
        </field>
    </record>
    <record model="ir.ui.view" id="medical_imaging_storage_tree_view">
        <field
            name="name"
        >medical.imaging.storage.tree (in medical_imaging_study)</field>
        <field name="model">medical.imaging.storage</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="medical_imaging_storage_act_window">
        <field name="name">Medical imaging Storage</field>
        <field name="res_model">medical.imaging.storage</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>
    <record model="ir.ui.menu" id="medical_imaging_storage_menu">
        <field name="name">Medical imaging Storage</field>
        <field name="parent_id" ref="medical_base.medical_root_configuration" />
        <field name="action" ref="medical_imaging_storage_act_window" />
        <field name="sequence" eval="98" />
    </record>
</odoo>
