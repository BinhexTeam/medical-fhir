<odoo>
    <record model="ir.ui.view" id="medical_imaging_endpoint_form_view">
        <field
            name="name"
        >medical.imaging.endpoint.form (in medical_imaging_study)</field>
        <field name="model">medical.imaging.endpoint</field>
        <field name="arch" type="xml">
            <form>
                <header>
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="connection_type" />
                        <field name="url" />
                        <field name="storage_id" />
                        <field name="user" />
                        <field name="password" />
                        <field name="tech_name" />
                        <field name="tz" />
                    </group>
                </sheet>
                <div class="oe_chatter" />
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="medical_imaging_endpoint_search_view">
        <field
            name="name"
        >medical.imaging.endpoint.search (in medical_imaging_study)</field>
        <field name="model">medical.imaging.endpoint</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
            </search>
        </field>
    </record>
    <record model="ir.ui.view" id="medical_imaging_endpoint_tree_view">
        <field
            name="name"
        >medical.imaging.endpoint.tree (in medical_imaging_study)</field>
        <field name="model">medical.imaging.endpoint</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="medical_imaging_endpoint_act_window">
        <field name="name">Medical Imaging Endpoint</field>
        <field name="res_model">medical.imaging.endpoint</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>
    <record model="ir.ui.menu" id="medical_imaging_endpoint_menu">
        <field name="name">Medical Imaging Endpoint</field>
        <field name="parent_id" ref="medical_base.medical_root_configuration" />
        <field name="action" ref="medical_imaging_endpoint_act_window" />
        <field name="sequence" eval="97" />
    </record>
</odoo>
